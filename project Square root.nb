Cell[BoxData[
 RowBox[{
  RowBox[{"provingLimit2", "[", 
   RowBox[{"b1_", ",", "b2_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"aN", ",", " ", 
      RowBox[{"steps", "=", "n"}], ",", " ", 
      RowBox[{"a1", "=", "b1"}], ",", 
      RowBox[{"a2", "=", "b2"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"l1", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"steps", ">", "1"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"aN", "=", 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"a1", "^", "2"}], "+", 
             RowBox[{"a2", "^", "2"}]}], ")"}], "/", "2"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"l1", ",", "aN"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"a1", "=", "a2"}], ";", "\[IndentingNewLine]", 
        RowBox[{"a2", "=", "aN"}], ";", "\[IndentingNewLine]", 
        RowBox[{"steps", "=", 
         RowBox[{"steps", "-", "1"}]}], ";"}]}], "\[IndentingNewLine]", "]"}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "l1", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{
  3.732468055505044*^9},ExpressionUUID->"ad8f114b-dbd1-4f4e-821b-\
010ae1151e36"],

