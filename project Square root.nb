Cell["Next we define the generalized limit.", "Text",
 CellChangeTimes->{{3.732310186060459*^9, 3.732310187008947*^9}, {
  3.732310260704073*^9, 3.732310266371866*^9}, {3.732361047597309*^9, 
  3.732361057328244*^9}},ExpressionUUID->"6e44842b-0407-4fd3-8e8e-\
c4f1b27da670"],

Cell[BoxData[
 RowBox[{
  RowBox[{"limitItself3", "[", 
   RowBox[{"a1_", ",", "a2_", ",", "x_"}], "]"}], " ", ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"a1", "^", "x"}], "+", 
         RowBox[{"2", "*", 
          RowBox[{"a2", "^", "x"}]}]}], ")"}], "/", "3"}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"1", "/", "x"}], ")"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",ExpressionUUID->"103d1fe5-ed6b-4602-a03a-f2a29720540d"],

