Cell[TextData[StyleBox["testLimit4 accepts a list of numbers as an argument \
such that it takes the first 2 numbers in the list and tests the equality of \
the sequence with these initial conditions to the proposed limit with the \
same initial conditions. When randomInt is input as the argument of \
testLimit, it means that the equality is tested for 50 sets of random initial \
conditions ranging from 1 to 100000000. As can be seen by the resulting list \
of \"True\" return values, for all 50 of these sets random initial \
conditions, the Root Mean Sequence at high n is equal to the proposed limit.",
 
 FontSize->16]], "Chapter",
 CellChangeTimes->{{3.732458689366996*^9, 3.732458712389695*^9}, {
   3.7324589383135633`*^9, 3.732458939177412*^9}, 
   3.732459009448758*^9},ExpressionUUID->"6c2b2473-a8f6-4a6a-9f8e-\
d436ecba3f37"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"randomInt4", "=", " ", 
   RowBox[{"RandomInteger", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"1", ",", "100000000"}], "}"}], ",", "50"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"testLimit4", "[", "l1_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"n", "<", "50"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"a1", "=", 
         RowBox[{"l1", "[", 
          RowBox[{"[", "n", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"a2", "=", 
         RowBox[{"l1", "[", 
          RowBox[{"[", 
           RowBox[{"n", "+", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
          RowBox[{"N", "[", 
           RowBox[{
            RowBox[{"provingLimit4", "[", 
             RowBox[{"a1", ",", "a2", ",", "100"}], "]"}], ",", "10"}], "]"}],
           "==", 
          RowBox[{"limitItself4", "[", 
           RowBox[{"a1", ",", "a2"}], "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"n", "=", 
         RowBox[{"n", "+", "1"}]}], ";"}]}], "\[IndentingNewLine]", "]"}]}]}],
    "\[IndentingNewLine]", "]"}]}]}], "Input",
