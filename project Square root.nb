Cell["\<\
First and foremost, we define our original recursive sequence that defines \
the Root Mean Square. This function which we call provingLimit takes 3 \
arguments: a position 1 for the sequence, a position 2 for the sequence, and \
a counter giving the maximum number iterations desired of the sequence. \
\>", "Text",
 CellChangeTimes->{{3.732306616036178*^9, 3.732306630856945*^9}, {
  3.732306710434896*^9, 3.732306740147819*^9}, {3.732306771078041*^9, 
  3.732306812660955*^9}, {3.732306842703107*^9, 
  3.73230699167369*^9}},ExpressionUUID->"5ba3b53d-81c3-40a6-971f-\
21a22093ea74"],

Cell[BoxData[
 RowBox[{
  RowBox[{"provingLimit", "[", 
   RowBox[{"b1_", ",", "b2_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"aN", ",", " ", 
      RowBox[{"steps", "=", "n"}], ",", " ", 
      RowBox[{"a1", "=", "b1"}], ",", 
      RowBox[{"a2", "=", "b2"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "This", " ", "loop", " ", "will", " ", "do", " ", "calculate", " ", 
      SubscriptBox["a", "n"]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"steps", ">", "1"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"aN", "=", 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"a1", "^", "2"}], "+", 
             RowBox[{"a2", "^", "2"}]}], ")"}], "/", "2"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"a1", "=", "a2"}], ";", "\[IndentingNewLine]", 
        RowBox[{"a2", "=", "aN"}], ";", "\[IndentingNewLine]", 
        RowBox[{"steps", "=", 
         RowBox[{"steps", "-", "1"}]}]}]}], "\[IndentingNewLine]", "]"}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "aN", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.732227029923731*^9, 3.732227075126217*^9}, {
  3.732227106863823*^9, 3.732227160630189*^9}, {3.73222721116334*^9, 
  3.732227222117577*^9}, {3.732227360431658*^9, 3.73222741747427*^9}, {
  3.73222756220677*^9, 3.732227564711556*^9}, {3.732227611513825*^9, 
  3.732227648378881*^9}, {3.732467822275278*^9, 
  3.732467846190584*^9}},ExpressionUUID->"66d7b9ea-aa4b-44ff-a1b1-\
e859edde25f4"],

