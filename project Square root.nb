Cell[TextData[{
 StyleBox["Let ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SubscriptBox["a", "n"]}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"fa342fb0-d35c-44ba-8ebb-2a208f3d1b4a"],
 StyleBox[")n\[GreaterEqual]1 be the sequence defined recursively by ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", 
    RowBox[{"n", "+", "1"}]], TraditionalForm]],
  FontSize->18,ExpressionUUID->"9537e67e-eeef-4c9e-ab84-c68c513226d5"],
 StyleBox[" = ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RadicalBox[
    FractionBox[
     RowBox[{
      SuperscriptBox[
       SubscriptBox["a", "n"], "3"], "+", 
      SuperscriptBox[
       SubscriptBox["a", 
        RowBox[{"n", "-", "1"}]], "3"]}], "2"], 
    RowBox[{"3", "  "}],
    MultilineFunction->None,
    SurdForm->True], TraditionalForm]],
  FontSize->18,ExpressionUUID->"84664c24-f3ae-46bd-982d-8709090faa9c"],
 StyleBox[" for n \[GreaterEqual] 2, with ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "1"], TraditionalForm]],
  FontSize->18,ExpressionUUID->"40ae1827-c936-402b-9f01-7ff941ab189e"],
 StyleBox[" and ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "2"], TraditionalForm]],
  FontSize->18,ExpressionUUID->"30f03849-9016-477d-81ee-1ae6eb364e8d"],
 StyleBox[" two positive numbers as initial values. Then ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   SubscriptBox["lim", 
    RowBox[{"n", "\[RightArrow]", "\[Infinity]"}]], TraditionalForm]],
  FontSize->18,ExpressionUUID->"2a248d8a-2690-4249-a660-28aafdd7c756"],
 StyleBox[" ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],
  FontSize->18,ExpressionUUID->"ea1661fd-f238-4cd4-9158-e086caa5ca60"],
 StyleBox[" =",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RadicalBox[
    FractionBox[
     RowBox[{
      SuperscriptBox[
       SubscriptBox["a", "1"], "3"], "+", 
      SuperscriptBox[
       SubscriptBox["a", "2"], "3"]}], "2"], 
    RowBox[{"3", "  "}],
    MultilineFunction->None,
    SurdForm->True], TraditionalForm]],
  FontSize->18,ExpressionUUID->"a0a347ce-b3ae-4baf-a6a5-736033a4d759"]
}], "Text",
