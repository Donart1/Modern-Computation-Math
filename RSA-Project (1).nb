Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"textToNums", "[", "text_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "numFromText", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"numFromText", "=", 
      RowBox[{"ToCharacterCode", "[", "text", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "pad", " ", "to", " ", "even", " ", "multiples", " ", "of", " ", "10"}],
       " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{"Length", "[", "numFromText", "]"}], ",", "10"}], "]"}], "!=",
         "0"}], ",", "\[IndentingNewLine]", 
       RowBox[{"AppendTo", "[", 
        RowBox[{"numFromText", ",", "32"}], "]"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", "numFromText"}]}], 
   "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"t1", "=", 
  RowBox[{"textToNums", "[", "\"\<hello world\>\"", "]"}]}]}], "Input",
