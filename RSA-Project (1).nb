 "If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"c", "=", 
    SuperscriptBox["m", "e"]}], TraditionalForm]],ExpressionUUID->
  "438d79df-8563-4bac-a9c8-24e2e54e7aab"],
