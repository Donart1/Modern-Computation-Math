Cell[BoxData[
 RowBox[{
  RowBox[{"encryptRSA", "[", 
   RowBox[{"text_", ",", "e_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"blocks", ",", " ", "k", ",", "i", ",", 
      RowBox[{"retList", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"blocks", "=", 
      RowBox[{"numsToBlocks", "[", " ", 
       RowBox[{"textToNums", "[", " ", "text", " ", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"k", "=", 
      RowBox[{"Length", "[", "blocks", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "k"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"AppendTo", "[", " ", 
         RowBox[{"retList", ",", 
          RowBox[{"modPow2", "[", " ", 
           RowBox[{
            RowBox[{"blocks", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ",", " ", "e", ",", " ", "n"}], 
           " ", "]"}]}], " ", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", "retList"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",ExpressionUUID->"ead3a317-1117-479f-8df7-dd22ae3cce2c"],

