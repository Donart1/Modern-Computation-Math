       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"tot", "=", 
        RowBox[{
         RowBox[{"1000", "*", "tot"}], " ", "+", 
         RowBox[{"numList", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}]}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "tot", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}], "\[IndentingNewLine]", 
